<meta name="twitter:title" content="<%= title_meta %>">
<meta name="twitter:description" content="<%= description_meta %>">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@<%= config[:twitter_owner] %>">
<meta name="twitter:creator" content="@<%= config[:twitter_creator] %>">
<meta name="twitter:domain" content="<%= config[:domain] %>">

<meta name="twitter:image" content="<%= image_path(config[:logo], absolute: true) %>">
<meta name="twitter:image:alt" content="<%= config[:company] %>">
